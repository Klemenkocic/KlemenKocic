{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "pnpm build",
  "outputDirectory": ".next",
  "ignoreCommand": "echo ok",
  "headers": [
    {
      "source": "/(.*)\.(?:js|css|woff2?|ttf|otf|eot|png|jpg|jpeg|gif|svg|webp|ico)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "redirects": [
    { "source": "https://www.klemenkocic.com/(.*)", "destination": "https://klemenkocic.com/$1", "permanent": true }
  ]
}


